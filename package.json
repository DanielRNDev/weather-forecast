{
  "name": "weather-forecast",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/**/**/*.{js,jsx}",
      "src/utils/**/*.{js,jsx}",
      "src/hooks/*.{js,jsx}",
      "src/routes/**/*.{js,jsx}",
      "src/contexts/**/*.{js,jsx}",
      "!**/node_modules/**"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "bootstrap": "5.0.2",
    "dayjs": "^1.10.6",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test-cover": "yarn test a --coverage",
    "coverage": "yarn test-cover && open coverage/lcov-report/index.html",
    "start-server": "node server/index.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/runtime": "^7.11.2",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.30.0",
    "eslint-plugin-react": "^7.24.0"
  }
}
